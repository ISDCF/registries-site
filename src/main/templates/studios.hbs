<!DOCTYPE html>
<html lang="en">
<header>
  {{>header}}
</header>
  <table class="table table-striped table-small table-hover">
    <thead class="thead-light">
      <tr> 
        <th scope="col" width="4em">Code</th>
        <th scope="col">Description</th>
      </tr>
    </thead>
    <tbody id="searchTable">
      {{#data}}
      <tr id="{{idType}}-{{code}}">
        <td class="code"><a class="anchor" id="{{code}}" href="#{{code}}"></a><code>{{code}}</code></td>
        <td><span class="description">{{description}}</span>
          {{#obsolete}}
            <span class="mx-3 badge badge-danger">Deprecated</span>
            {{#if obsoletedBy}}
              <span>Replaced by:</span>
            {{/if}}
            {{#obsoletedBy}}
              <a class="{{idType}}-link" id="clear-filter" href="#{{this}}">{{this}}</a>
            {{/obsoletedBy}}
          {{/obsolete}}
        </td>
      </tr>
      {{/data}}
    </tbody>
  </table>
  <footer>
    {{>footer}}
  </footer>
</html>