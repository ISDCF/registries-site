<!DOCTYPE html>
<html lang="en">
{{>header}}
  <!-- Markdown content starts -->
{{>content}}
  <!-- Markdown content ends -->
  <!-- Registry content starts - do not edit above this line -->
          <table id="sorttable" class="table table-small table-hover display">
            <thead id="tableheader" class="thead-light">
              <tr>
                <th scope="col" class="align-text-top">Territory</th>
                <th scope="col" class="align-text-top">DCNC Code<br><small>(For CTT)</small></th>
                <th scope="col" class="align-text-top">ReleaseTerritory Tag<br><small>(For DCP Metadata)</small></th>
                <th scope="col" class="align-text-top">Tag Scope<br><small>(For DCP Metadata)</small></th>
                <th scope="col" class="align-text-top text-wrap col-2">Notes</th>
              </tr>
            </thead>
            <tbody id="searchTable">
              {{#data}}
              <tr id="{{../idType}}-{{tag}}">
                <td>
                  <span class="description">{{dcncTerritory}}</span>
                </td>
                <td class="code">
                  {{#if dcncTag}}
                  <code>{{dcncTag}}</code>
                  {{else}}
                  <em>n/a</em>
                  {{/if}}
                </td>
                <td class="rfc">
                  <a class="anchor" id="{{tag}}" href="#{{tag}}"></a><code>{{tag}}</code>
                </td>
                <td class="cell-breakAll">
                  {{#if tagScope}}
                  <a target="_blank" href="{{tagScope}}"><code>{{tagScope}}</code></a>
                  {{/if}}
                </td>
                <td class="comment">
                  {{#comments}}
                  <small><em>{{this}}</em></small>
                  {{/comments}}
                </td>
              </tr>
              {{/data}}
            </tbody>
          </table>
<!-- Main content ends - do not edit below this line -->
{{>footer}}

</html>