<!DOCTYPE html>
<html lang="en">

  {{>header}}
  <!-- Main content starts - do not edit above this line -->
    <table id="sorttable" class="table table-small table-hover display">
      <thead class="thead-light">
        <tr>
          <th scope="col" class="align-text-top">Territory</th>
          <th scope="col" class="align-text-top">DCNC Code <small>(For CTT)</small></th>
          <th scope="col" class="align-text-top">ReleaseTerritory Tag <small>(For DCP Metadata)</small></th>
          <th scope="col" class="align-text-top">Tag Scope</th>
          <th scope="col" class="align-text-top text-wrap" width="20%">Comments</th>
        </tr>
      </thead>
      <tbody id="searchTable">
        {{#data}}
        <tr id="{{../idType}}-{{rfc5646Tag}}">
          <td><span class="description">{{dcncTerritory}}</span></td>
          <td class="code">
            {{#if dcncTag}}
            <code>{{dcncTag}}</code>
            {{else}}
            <em>n/a</em>
            {{/if}}
          </td>
          <td class="rfc"><a class="anchor" id="{{rfc5646Tag}}" href="#{{rfc5646Tag}}"></a><code class="sampCode">{{tag}}</code></td>
          <td>
            {{#if tagScope}}
            <a target="_blank" href="{{tagScope}}"><code>{{tagScope}}</code></a>
            {{/if}}
          </td>
          <td class="comment">
            {{#comments}}
                <small>{{this}}</small>
            {{/comments}}
          </td>
        </tr>
        {{/data}}
      </tbody>
    </table>
<!-- Main content ends - do not edit below this line -->
{{>footer}}

</html>